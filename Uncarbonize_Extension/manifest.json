{
  "description": "Self-Adaptable HTML engine making a trage-off between energy consumption and quality of page rendering",
  "manifest_version": 2,
  "name": "Uncarbonize",
  "version": "1.0",
  "icons": {
    "48": "icon/on.svg",
    "96": "icon/on.svg"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "storage",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "uncarbonize.js"
    ]
  },
  "browser_specific_settings": {
    "gecko": {
      "strict_min_version": "57.0a1"
    }
  },
  "options_ui": {
    "page": "options/options.html",
    "browser_style": true
  },
  "browser_action": {
    "default_icon": "icon/on.svg",
    "theme_icons": [
      {
        "light": "icon/on.svg",
        "dark": "icon/on.svg",
        "size": 32
      }
    ],
    "default_title": "Uncarbonize",
    "default_popup": "options/options.html"
  }
}