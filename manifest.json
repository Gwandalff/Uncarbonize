{
  "description": "Self-Adaptable HTML interpreter",
  "manifest_version": 2,
  "name": "Carbonize",
  "version": "1.0",

  "permissions": [
    "webRequest", "webRequestBlocking", "<all_urls>"
  ],

  "background": {
    "scripts": ["log.js"]
  },

  "browser_specific_settings": {
    "gecko": {
      "strict_min_version": "57.0a1"
    }
  }

}
